<!--pages/manager/msg_success.wxml-->
<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">{{operate}}成功</view>
            <view class="weui-msg__desc"></view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary" bindtap='continueAdd'>继续添加</button>
                <button class="weui-btn" type="default" bindtap='back'>返回列表</button>
            </view>
        </view>
    </view>
</view>