<!--pages/manager/manager.wxml-->
<import src="../common/UserTop.wxml" />
<view class='container'>
  <!--登录信息start  -->
  <template is="UserTop" data="{{userInfo,manager}}" />
  <!--登录信息end  -->
  <!--列表start  -->
  <view class="weui-cells__title">后台操作</view>
  <view class="weui-cells weui-cells_after-title">
    <navigator url="swiper" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">轮播图</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">首页</view>
    </navigator>
    <navigator url="goods_list" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">产品列表</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">添加、删除</view>
    </navigator>
  </view>
  <!--列表end  -->
  <view class="weui-footer weui-footer_fixed-bottom">
    <view class="weui-footer__links">
      <navigator url="../index/index" open-type='switchTab' class="weui-footer__link">返回首页</navigator>
    </view>
  </view>
</view>